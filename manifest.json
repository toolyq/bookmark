{
    "manifest_version": 2,

    "name": "书签同步",
    "description": "上传/下载书签信息到Github",
    "version": "1.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "书签同步"
    },
    "permissions": [
        "bookmarks",
        "storage",
        "https://api.github.com/"
    ]
}